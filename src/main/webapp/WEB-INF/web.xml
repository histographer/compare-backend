<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Compare Backend</display-name>
  
  <!-- In order to ensure that listeners are notified in a certain order, they need to be specified in this file -->
  <listener>
    <listener-class>no.digipat.compare.listeners.MongoDBContextListener</listener-class>
  </listener>
  <listener>
    <listener-class>no.digipat.compare.listeners.ImageRetrievalContextListener</listener-class>
  </listener>
  
</web-app>
